ada"}, {"api_detail_url": "http://comicvine.gamespot.com/api/person/4040-12374/", "site_detail_url": "http://comicvine.gamespot.com/karl-moline/4040-12374/", "role": "penciler", "id": 12374, "name": "Karl Moline"}, {"api_detail_url": "http://comicvine.gamespot.com/api/person/4040-7169/", "site_detail_url": "http://comicvine.gamespot.com/mark-pennington/4040-7169/", "role": "inker", "id": 7169, "name": "Mark Pennington"}, {"api_detail_url": "http://comicvine.gamespot.com/api/person/4040-41683/", "site_detail_url": "http://comicvine.gamespot.com/molly-lazer/4040-41683/", "role": "editor", "id": 41683, "name": "Molly Lazer"}, {"api_detail_url": "http://comicvine.gamespot.com/api/person/4040-10388/", "site_detail_url": "http://comicvine.gamespot.com/rus-wooton/4040-10388/", "role": "letterer", "id": 10388, "name": "Rus Wooton"}, {"api_detail_url": "http://comicvine.gamespot.com/api/person/4040-41609/", "site_detail_url": "http://comicvine.gamespot.com/tom-brevoort/4040-41609/", "role": "editor", "id": 41609, "name": "Tom Brevoort"}], "first_appearance_locations": [{"api_detail_url": "http://comicvine.gamespot.com/api/location/4020-56019/", "site_detail_url": "http://comicvine.gamespot.com/los-angeles/4020-56019/", "id": 56019, "name": "Los Angeles"}], "issue_number": "6", "first_appearance_teams": [{"api_detail_url": "http://comicvine.gamespot.com/api/team/4060-40606/", "site_detail_url": "http://comicvine.gamespot.com/loners/4060-40606/", "id": 40606, "name": "Loners"}]}
